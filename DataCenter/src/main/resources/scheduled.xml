		<task:scheduled ref="HDB_KCJY_Task" method="run" cron="0 0 14 * * ?" />
		<task:scheduled ref="HDB_MDHD_Task" method="run" cron="0 0 15 * * ?" />
		<task:scheduled ref="HDB_QZHD_Task" method="run" cron="0 0 16 * * ?" />
		<task:scheduled ref="HDB_WHYJ_Task" method="run" cron="0 0 17 * * ?" />
