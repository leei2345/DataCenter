		<task:scheduled ref="dianPing_SHFW_SWL_Task" method="run" cron="0 40 4 19 * ?" />
		<task:scheduled ref="dianPing_SHFW_XQ_SWL_Task" method="run" cron="0 40 5 19 * ?" />
		<task:scheduled ref="dianPing_SHFW_YY_Task" method="run" cron="0 40 6 19 * ?" />



	</task:scheduled-tasks>


</beans>
