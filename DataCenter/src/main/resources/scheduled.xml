		<task:scheduled ref="proxyCheckTask" method="startCheck" cron="0 0 * * * ?" />
		<task:scheduled ref="SogouCookieTask" method="run" cron="0 0 9-20 * * ?" />
	</task:scheduled-tasks>


</beans>
