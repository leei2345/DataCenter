		<task:scheduled ref="dianPing_JIUD_WXJJD_Task" method="run" cron="0 0 0 19 * ?" />
		<task:scheduled ref="dianPing_JD_CSGG_Task" method="run" cron="0 0 12 19 * ?" />
		<task:scheduled ref="dianPing_JIUD_SXJJD_Task" method="run" cron="0 0 12 20 * ?" />
		
 		<task:scheduled ref="baiduTask" method="run" cron="0 10 10,18 * * ?" />
