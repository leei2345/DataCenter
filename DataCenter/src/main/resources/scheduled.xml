	<bean id="dianPing_SHFW_XQ_SWL_Task" class="com.jinba.scheduled.DianPing_SHFW_XQ_SWL_Task"></bean>
	<bean id="dianPing_SHFW_YY_Task" class="com.jinba.scheduled.DianPing_SHFW_YY_Task"></bean>


	<task:scheduled-tasks>

		<task:scheduled ref="proxyCheckTask" method="run" cron="0 0,30 * * * ?" />
		<task:scheduled ref="dianPing_GW_ZHSC_Task" method="run" cron="0 20 0 19 * ?" />
		<task:scheduled ref="dianPing_JD_CSGG_Task" method="run" cron="0 20 1 19 * ?" />
		<task:scheduled ref="dianPing_JD_DZWY_Task" method="run" cron="0 20 2 19 * ?" />
		<task:scheduled ref="dianPing_JD_GZ_Task" method="run" cron="0 20 3 19 * ?" />
		<task:scheduled ref="dianPing_JD_MSGJ_Task" method="run" cron="0 20 4 19 * ?" />
		<task:scheduled ref="dianPing_JD_YLY_Task" method="run" cron="0 20 5 19 * ?" />
		<task:scheduled ref="dianPing_JD_ZGZL_Task" method="run" cron="0 20 6 19 * ?" />
		<task:scheduled ref="dianPing_JD_ZRFG_Task" method="run" cron="0 20 7 19 * ?" />
		<task:scheduled ref="dianPing_JIUD_3XJJD_Task" method="run" cron="0 20 8 19 * ?" />
